{
  "name": "booth-videos",
  "version": "0.1.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@heroku/react-malibu": "^4.1.0",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.7",
    "@types/react-dom": "^16.9.2",
    "@types/react-router-dom": "^5.1.0",
    "idb-keyval": "^3.2.0",
    "if-env": "^1.0.4",
    "node-sass": "^4.12.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.2.0",
    "serve": "^11.2.0",
    "typescript": "^3.6.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": ["build/*"],
  "private": true,
  "scripts": {
    "build": "echo \"REACT_APP_VIDEOS=${REACT_APP_VIDEOS}\" && react-scripts build",
    "eject": "react-scripts eject",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "react-scripts start",
    "start:prod": "serve --single --listen tcp://0.0.0.0:$PORT build",
    "test": "react-scripts test"
  }
}
